<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    	http://www.springframework.org/schema/context
    	http://www.springframework.org/schema/context/spring-context-3.0.xsd"
       default-autowire="byName" default-lazy-init="false">

<context:annotation-config/>
<context:component-scan base-package="com.treeinjar.base"/>
<context:component-scan base-package="com.treeinjar.model.dao"/>
<context:component-scan base-package="com.treeinjar.model.dto"/>
<context:component-scan base-package="com.treeinjar.model.mybatis.mapper"/>
<context:component-scan base-package="com.treeinjar.service.service"/>
<context:component-scan base-package="com.treeinjar.service.serviceimpl"/>


</beans>
